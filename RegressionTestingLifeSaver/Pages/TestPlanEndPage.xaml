<Page x:Class="RegressionTestingLifeSaver.Pages.TestPlanEndPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RegressionTestingLifeSaver.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="1000" d:DesignWidth="1200"
      Title="TestPlanEndPage">

    <Border Padding="20 10" Background="#E0E0E0">
        <StackPanel VerticalAlignment="Center">
            <TextBlock HorizontalAlignment="Center" FontSize="20" Text="Congrats you're done with this test plan." />
            <Button HorizontalAlignment="Center" Width="50" Margin="0 40 0 10" Content="Back" Click="MovePreviousButton_Click" />
            <Button HorizontalAlignment="Center" Width="100" Content="Upload to HSIDB"/>
        </StackPanel>
    </Border>
</Page>
